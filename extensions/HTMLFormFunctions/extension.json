	},
	"ExtensionMessagesFiles": {
		"HTMLFormFunctionsMagic": "HTMLFormFunctions.i18n.php"
	},
	"MessagesDirs": {
		"HTMLFormFunctions": [
			"i18n"
		]
	},
	"manifest_version": 2
}
